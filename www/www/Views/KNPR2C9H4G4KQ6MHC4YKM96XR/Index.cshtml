  <script type="text/javascript">
      function getInventoryStatus(pid) {
        $('.dr_checkInventory').after('<span class="dr_retrievingInventoryInfo"> Loading</span>');
        var url = "/store/msapac/en_GB/DisplayPage/id.ProductInventoryStatusXmlPage/productID."+pid;
        getAvailability(url);
      }
      function createInfo(data) {
        var status = data.getElementsByTagName("inventoryStatus")[0].firstChild.nodeValue;
        if (status=='PRODUCT_INVENTORY_OUT_OF_STOCK') {
          var status = 'Out of Stock';
        } else if (status=='PRODUCT_INVENTORY_IN_STOCK') {
          var status = 'In Stock';
        } else if (status=='PRODUCT_INVENTORY_BACKORDERED') {
          var status = 'BACKORDERED';
        } else {
          var status = '';
        }
        var estimated = data.getElementsByTagName("isStatusEstimated")[0].firstChild.nodeValue;
        var quantity = data.getElementsByTagName("availableQuantity")[0].firstChild.nodeValue;
        $('.dr_invStatus').html(status);
        $('.dr_inventoryAvailableQuantity').html(quantity);
        $('.dr_retrievingInventoryInfo').hide();
      }
      function getAvailability(url){
        $.get(url, function(data) {
          createInfo(data);
        });
      }
      $('.dr_checkInventory').click(function () {
        if ( $('#dr_purchaseDetails select').length && $('#dr_purchaseDetails select option:selected').attr('dataType') == 'false' )  {getInventoryStatus($('#dr_purchaseDetails select option:selected').val())  } 
        else if ( $('#dr_purchaseDetails input[type="radio"]').length && $('input[type="radio"]:checked').attr('dataType') == 'false' ) {getInventoryStatus($('input[type="radio"]:checked').val())  }
        else if ( $('#dr_purchaseDetails input[type="hidden"][name="productID"]').length && $('#dr_purchaseDetails input[type="hidden"][name="productID"]').attr('dataType') == 'false'  ) {getInventoryStatus($('#dr_purchaseDetails input[type="hidden"][name="productID"]').val()) }
        else {
        $('.dr_invStatus').html(' ');
        $('.dr_inventoryAvailableQuantity').html(' ');
        $('.dr_retrievingInventoryInfo').hide();
        }
      }).attr('href','javascript:void(0)');
    </script>
