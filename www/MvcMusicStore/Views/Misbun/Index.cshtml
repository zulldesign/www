@model IEnumerable<MvcMusicStore.Models.Kategori>
@{
    ViewBag.Title = "Misbun";
}
<h3>Browse Kategoris</h3>

<p>
    Select from @Model.Count() kategoris:</p>
<ul>
    @foreach (var kategori in Model)
    {
        <li>@Html.ActionLink(kategori.Nama, "Browse", new { kategori = kategori.Nama })</li>
    }
</ul>
