<?xml version="1.0" standalone="yes"?>
<paypalshopbuilder date="2007/1/15" version="2.0.0.1" desc="default template" author="NTK Software">
	<!-- copy menu.js -->
	<control id="menu.js" type="copy"
		ifiles="menu.js"/>
	<!-- copy ntkcartpp.js -->
	<control id="ntkcartpp.js" type="copy"
		ifiles="ntkcartpp.js"/>
	<!-- copy ntkcart.js -->
	<control id="ntkcart.js" type="copy"
		ifiles="ntkcart.js"/>
	<!-- copy all images from the images folder -->
	<control ofolder="images" type="copy" id="images" ifolder="images"/>
	<!-- list page pager -->
	<control type="include" id="pager" ifiles="list-pager.htm">
		<session type="key" value="pager"/>
	</control>
	<!-- generate list page -->
	<control ofile="list" oext="*" type="table" id="list" ifiles="list.htm"
			cond="table/TblList/EQ/True">
		<!-- page header -->
		<session type="key" value="header" ifile="template.htm"/>
		<!-- site path -->
		<session type="key" value="site-path" ifile="list-sitepath.htm"/>
		<!-- list pager -->
		<session type="include" value="pager"
			cond="proj/TopPageLink/EQ/True"/>
		<!-- grid layout -->
		<session type="key" value="table-start"/>
		<session type="key" value="multi-tr-start" ifile="list-multi.htm"
			cond="PROJ/RecPerRow/GE/1"/>
		<session type="key" value="grid-tr-start"
			cond="PROJ/RecPerRow/LT/1"/>
		<session type="key" value="paypal-start" ifile="paypal.htm"/>
		<session type="key" value="multi-td-start" ifile="list-multi.htm"
			cond="PROJ/RecPerRow/GE/1"/>
		<session type="key" value="grid-td-start"
			cond="PROJ/RecPerRow/LT/1"/>
		<session type="key" value="paypal-content-hidden" ifile="paypal.htm"/>
		<session type="key" value="table-content"/>
		<session type="key" value="multi-td-end" ifile="list-multi.htm"
			cond="PROJ/RecPerRow/GE/1"/>
		<session type="key" value="grid-td-end"
			cond="PROJ/RecPerRow/LT/1"/>
		<session type="key" value="paypal-end" ifile="paypal.htm"/>
		<session type="key" value="multi-tr-end" ifile="list-multi.htm"
			cond="PROJ/RecPerRow/GE/1"/>
		<session type="key" value="grid-tr-end"
			cond="PROJ/RecPerRow/LT/1"/>
		<session type="key" value="table-end"/>
		<!-- list pager -->
		<session type="include" value="pager"
			cond="proj/BottomPageLink/EQ/True"/>
		<!-- page footer -->
		<session type="key" value="footer" ifile="template.htm"/>
	</control>
	<!-- generate view page -->
	<control ofile="view" oext="*" type="table" id="view" ifiles="view.htm"
			cond="table/TblView/EQ/True">
		<!-- page header -->
		<session type="key" value="header" ifile="template.htm"/>
		<!-- general layout -->
		<session type="key" value="table-start"/>
		<session type="key" value="paypal-start" ifile="paypal.htm"/>
		<session type="key" value="paypal-content-hidden" ifile="paypal.htm"/>
		<session type="key" value="table-content"/>
		<session type="key" value="paypal-end" ifile="paypal.htm"/>
		<session type="key" value="table-end"/>
		<!-- page footer -->
		<session type="key" value="footer" ifile="template.htm"/>
	</control>
	<!-- generate menu page -->
	<control ofile="menu" oext="*" type="table" id="menu" ifiles="menu.htm"
			cond="table/TblMenu/EQ/True">
		<!-- page header -->
		<session type="key" value="header" ifile="template.htm"/>
		<!-- default layout -->
		<session type="key" value="menucontent"/>
		<!-- page footer -->
		<session type="key" value="footer" ifile="template.htm"/>
	</control>
	<!-- generate checkout page -->
	<control ofile="checkout" oext="*" type="other" id="checkout" ifiles="checkout.htm"
			cond="#Product/TblppType/EQ/3">
		<!-- page header -->
		<session type="key" value="header" ifile="template.htm"/>
		<!-- checkout page content -->
		<session type="key" value="checkout"/>
		<!-- page footer -->
		<session type="key" value="footer" ifile="template.htm"/>
	</control>
	<!-- generate shipping page -->
	<control ofile="shipping" oext="*" type="other" id="shipping" ifiles="shipping.htm"
			cond="#Product/TblppType/EQ/3">
		<!-- page header -->
		<session type="key" value="header" ifile="template.htm"/>
		<!-- shipping page content -->
		<session type="key" value="shipping"/>
		<!-- page footer -->
		<session type="key" value="footer" ifile="template.htm"/>
	</control>
	<!-- generate confirm page -->
	<control ofile="confirm" oext="*" type="other" id="confirm" ifiles="confirm.htm"
			cond="#Product/TblppType/EQ/3">
		<!-- page header -->
		<session type="key" value="header" ifile="template.htm"/>
		<!-- confirm page content -->
		<session type="key" value="confirm"/>
		<!-- page footer -->
		<session type="key" value="footer" ifile="template.htm"/>
	</control>
	<!-- generate default page -->
	<control ofile="*" oext="*" type="other" id="default" ifiles="index.htm">
		<!-- default layout -->
		<session type="key" value="htm-begin"/>
	</control>
	<!-- generate ntkcartconfig.js -->
	<control ofile="ntkcartconfig" oext="js" type="other" id="ntkcartconfig" ifiles="ntkcartconfig.js">
		<session type="key" value="ntkcartconfig"/>
	</control>
	<!-- generate ntkmenuconfig.js -->
	<control ofile="ntkmenuconfig" oext="js" type="other" id="ntkmenuconfig" ifiles="ntkmenuconfig.js">
		<session type="key" value="ntkmenuconfig"/>
	</control>
	<!-- generate ntkcategoryconfig.js -->
	<control ofile="ntkcategoryconfig" oext="js" type="other" id="ntkcategoryconfig" ifiles="ntkcategoryconfig.js">
		<session type="key" value="ntkcategoryconfig"/>
	</control>
	<!-- generate autorun.ini -->
	<control ofile="autorun" oext="ini" type="other" id="autorun" ifiles="autorun.ini"
			cond="proj/GenAutoRun/EQ/True">
		<session type="key" value="autorun"/>
	</control>
	<!-- create dummy index.html to server/download -->
	<control ofolder="server/download" ofile="index" oext="*" id="index" type="other"
			ifiles="dummy.htm"
			cond="proj/DigitalDownload/EQ/True">
		<session type="key" value="index"/>
	</control>	
	<!-- *** Server Testing Scripts *** -->
	<control ofolder="server" id="index.html" type="copy"
			ifolder="server" ifiles="index.html"/>
	<control ofile="server/testemail" oext="htm" type="other" id="testemail" ifiles="server/testemail.htm">
		<session type="key" value="testemail"/>
	</control>	
	<control ofolder="server" id="testemail.asp" type="copy"
			ifolder="server" ifiles="testemail.asp"/>
	<control ofolder="server" id="testemail.php" type="copy"
			ifolder="server" ifiles="testemail.php"/>
	<control ofolder="server" id="testpath.asp" type="copy"
			ifolder="server" ifiles="testpath.asp"/>
	<control ofolder="server" id="testpath.php" type="copy"
			ifolder="server" ifiles="testpath.php"/>
	<control ofolder="server" id="testserver.asp" type="copy"
			ifolder="server" ifiles="testserver.asp"/>
	<control ofolder="server" id="testserver.php" type="copy"
			ifolder="server" ifiles="testserver.php"/>
	<control ofolder="server" id="testphpinfo.php" type="copy"
			ifolder="server" ifiles="testphpinfo.php"/>
	<!-- *** ASP scripts *** -->
	<!-- generate config.asp -->
	<control ofolder="server/asp" ofile="config" oext="asp" type="other" id="config"
			ifolder="server/asp" ifiles="config.asp"
			cond="proj/ServerSideScript/EQ/ASP">
		<session type="key" value="config"/>
	</control>
	<!-- copy ntkfn.asp -->
	<control ofolder="server/asp" id="ntkfn.asp" type="copy"
			ifolder="server/asp" ifiles="ntkfn.asp"
			cond="proj/ServerSideScript/EQ/ASP"/>
	<!-- copy ipn.asp -->
	<control ofolder="server/asp" id="ipn.asp" type="copy"
			ifolder="server/asp" ifiles="ipn.asp"
			cond="proj/ServerSideScript/EQ/ASP"/>
	<!-- copy ipn.txt -->
	<control ofolder="server/asp" id="ipn.txt" type="copy"
			ifolder="server/asp" ifiles="ipn.txt"
			cond="proj/ServerSideScript/EQ/ASP"/>
	<!-- copy approval.asp -->
	<control ofolder="server/asp" id="approval.asp" type="copy"
			ifolder="server/asp" ifiles="approval.asp"
			cond="proj/ServerSideScript/EQ/ASP,proj/DownloadApproval/EQ/True"/>
	<!-- copy approval.txt -->
	<control ofolder="server/asp" id="approval.txt" type="copy"
			ifolder="server/asp" ifiles="approval.txt"
			cond="proj/ServerSideScript/EQ/ASP,proj/DownloadApproval/EQ/True"/>
	<!-- generate pdt.asp -->
	<control ofolder="server/asp" ofile="pdt" oext="asp" type="other" id="pdt"
			ifolder="server/asp" ifiles="pdt.asp"
			cond="proj/ServerSideScript/EQ/ASP">
		<!-- pdt include -->
		<session type="key" value="pdt_include"/>
		<!-- page header -->
		<session type="key" value="header" ifile="template.htm"/>
		<!-- pdt page content -->
		<session type="key" value="pdt_content"/>
		<!-- page footer -->
		<session type="key" value="footer" ifile="template.htm"/>
	</control>
	<!-- copy purge.asp -->
	<control ofolder="server/asp" id="purge.asp" type="copy"
			ifolder="server/asp" ifiles="purge.asp"
			cond="proj/ServerSideScript/EQ/ASP,proj/DigitalDownload/EQ/True"/>
	<!-- create dummy index.html to server/asp -->
	<control ofolder="server/asp" ofile="index" oext="*" id="index" type="other"
			ifiles="dummy.htm"
			cond="proj/ServerSideScript/EQ/ASP">
		<session type="key" value="index"/>
	</control>	
	<!-- *** PHP scripts *** -->
	<!-- copy phpmailer folder -->
	<control ofolder="server/php/phpmailer" type="copy" id="phpmailer" ifolder="server/php/phpmailer"
			cond="proj/ServerSideScript/EQ/PHP"/>
	<!-- generate config.php -->
	<control ofolder="server/php" ofile="config" oext="php" type="other" id="config"
			ifolder="server/php" ifiles="config.php"
			cond="proj/ServerSideScript/EQ/PHP">
		<session type="key" value="config"/>
	</control>
	<!-- copy ntkfn.php -->
	<control ofolder="server/php" id="ntkfn.php" type="copy"
			ifolder="server/php" ifiles="ntkfn.php"
			cond="proj/ServerSideScript/EQ/PHP"/>
	<!-- copy ipn.php -->
	<control ofolder="server/php" id="ipn.php" type="copy"
			ifolder="server/php" ifiles="ipn.php"
			cond="proj/ServerSideScript/EQ/PHP"/>
	<!-- copy ipn.txt -->
	<control ofolder="server/php" id="ipn.txt" type="copy"
			ifolder="server/php" ifiles="ipn.txt"
			cond="proj/ServerSideScript/EQ/PHP"/>
	<!-- copy approval.php -->
	<control ofolder="server/php" id="approval.php" type="copy"
			ifolder="server/php" ifiles="approval.php"
			cond="proj/ServerSideScript/EQ/PHP,proj/DownloadApproval/EQ/True"/>
	<!-- copy approval.txt -->
	<control ofolder="server/php" id="approval.txt" type="copy"
			ifolder="server/php" ifiles="approval.txt"
			cond="proj/ServerSideScript/EQ/PHP,proj/DownloadApproval/EQ/True"/>
	<!-- generate pdt.php -->
	<control ofolder="server/php" ofile="pdt" oext="php" type="other" id="pdt"
			ifolder="server/php" ifiles="pdt.php"
			cond="proj/ServerSideScript/EQ/PHP">
		<!-- pdt include -->
		<session type="key" value="pdt_include"/>
		<!-- page header -->
		<session type="key" value="header" ifile="template.htm"/>
		<!-- pdt page content -->
		<session type="key" value="pdt_content"/>
		<!-- page footer -->
		<session type="key" value="footer" ifile="template.htm"/>
	</control>
	<!-- copy purge.php -->
	<control ofolder="server/php" id="purge.php" type="copy"
			ifolder="server/php" ifiles="purge.php"
			cond="proj/ServerSideScript/EQ/PHP,proj/DigitalDownload/EQ/True"/>	
	<!-- create dummy index.html to server/php -->
	<control ofolder="server/php" ofile="index" oext="*" id="index" type="other"
			ifiles="dummy.htm"
			cond="proj/ServerSideScript/EQ/PHP">
		<session type="key" value="index"/>
	</control>	
</paypalshopbuilder>
